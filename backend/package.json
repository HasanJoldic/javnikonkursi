    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib",
    "compile": "babel src --out-dir lib --extensions '.ts'",
    "compile:watch": "npm run compile --watch",
    "build": "npm run clean && npm run compile",
    "build:watch": "babel src --out-dir lib --extensions '.ts' --watch",
    "start": "node lib/app/index.js",
    "start:watch": "DEBUG=express:* nodemon lib/app/index.js",
    "pm2:start": "NODE_ENV=production pm2 start lib/app/index.js --name konkursi-backend --log pm2.log",
    "pm2:restart": "NODE_ENV=production pm2 restart konkursi-backend --log pm2.log",
    "migrate:latest": "knex --cwd=./lib/db/migrations --knexfile=../knexfile.js migrate:latest",
    "tsc": "tsc"
